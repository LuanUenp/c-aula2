#include <iostream>
using namespace std;

int main(){
  int arr[5], repetidos[5];
  int x = 0, y = 1;

  for (int i = 0; i < 5; i++){
    repetidos[i] = -1;
  }
  
  cout << "Digite todos os elementos do vetor: " << endl;
  for(int i = 0; i < 5; i++){
      cin >> arr[i];
  }
  
  for(int i = 0; i < 5; i++){
    for(int j = y; j < 5; j++){
      if(arr[i] == arr[j]){
        for(int k = 0; repetidos[k] != arr[i] && k < 5; k++) {
          if(k == 4) repetidos[x] = arr[i];
        }  
          x++;
      }
    }
    y++;
  }
  cout << "Os elementos repetidos sÃ£o: " << endl;
  for(int i = 0; i < x; i++){
    if(repetidos[i] != -1)
    cout << repetidos[i] << endl;
  }

  return 0;
}
