#include <iostream>
using namespace std;

int main(){
  int v [8];
  
  cout << "Digite todos os elementos do vetor: " << endl;
  for(int i = 0; i < 8; i++){
    cin >> v[i];
  }
  
  cout << endl;
  for(int i = 0; i < 8; i++){
    cout << v[i] << " ";
  }
  
  cout << endl << endl;
  cout << "Vetor invertido: " << endl;
  for(int i = 7; i >= 0; i--){
    cout << v[i] << " ";
  }
  return 0;
}
